{
  "name": "d3-horizon",
  "version": "1.11.2",
  "description": "An horizon area chart D3 component",
  "license": "MIT",
  "type": "module",
  "unpkg": "dist/d3-horizon.min.js",
  "jsdelivr": "dist/d3-horizon.min.js",
  "main": "dist/d3-horizon.mjs",
  "module": "dist/d3-horizon.mjs",
  "exports": {
    "umd": "./dist/d3-horizon.min.js",
    "default": "./dist/d3-horizon.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/d3-horizon.git"
  },
  "homepage": "https://github.com/vasturiano/d3-horizon",
  "keywords": [
    "d3",
    "horizon",
    "area",
    "chart"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/d3-horizon/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -w -c",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "accessor-fn": "1",
    "d3-array": "1 - 3",
    "d3-scale": "1 - 4",
    "d3-selection": "2 - 3",
    "d3-shape": "1 - 3",
    "d3-transition": "2 - 3",
    "index-array-by": "1",
    "kapsule": "1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "engines": {
    "node": ">=12"
  }
}
